{
  "name": "@ai-coders/context",
  "version": "0.4.0",
  "description": "CLI tool for generating codebase documentation and AI agent prompts",
  "main": "dist/index.js",
  "bin": {
    "ai-context": "dist/index.js"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE",
    "prompts/**/*"
  ],
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "test": "jest",
    "sync:env:app": "node scripts/sync-app-env.mjs",
    "build:app": "npm run sync:env:app && npm --prefix mapa-app run build",
    "dev:app": "npm run sync:env:app && npm --prefix mapa-app run dev",
    "preview:app": "npm run sync:env:app && npm --prefix mapa-app run preview",
    "clickhouse:up": "docker compose -f infra/clickhouse/docker-compose.yml up -d",
    "clickhouse:down": "docker compose -f infra/clickhouse/docker-compose.yml down",
    "clickhouse:logs": "docker compose -f infra/clickhouse/docker-compose.yml logs -f",
    "clickhouse:ps": "docker compose -f infra/clickhouse/docker-compose.yml ps",
    "clickhouse:health": "node scripts/clickhouse-health.mjs",
    "clickhouse:sql": "docker compose -f infra/clickhouse/docker-compose.yml exec -T clickhouse clickhouse-client --user ${CLICKHOUSE_USER:-mapa_app} --password ${CLICKHOUSE_PASSWORD:-mapa_local_dev_password} --query",
    "clickhouse:provision-user": "node scripts/clickhouse-provision-user.mjs",
    "syn:middleware": "node scripts/syn-middleware.mjs",
    "syn:ingest": "node scripts/syn-ingest-semantic-layer.mjs",
    "prepublishOnly": "npm run build",
    "version": "npm run build",
    "release": "npm version patch && npm publish --access public",
    "release:minor": "npm version minor && npm publish --access public",
    "release:major": "npm version major && npm publish --access public"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vinilana/ai-coders-context.git"
  },
  "homepage": "https://github.com/vinilana/ai-coders-context#readme",
  "bugs": {
    "url": "https://github.com/vinilana/ai-coders-context/issues"
  },
  "keywords": [
    "cli",
    "documentation",
    "agents",
    "scaffold"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/claude-code": "^2.1.58",
    "@google/gemini-cli": "^0.31.0-preview.0",
    "audit": "^0.0.6",
    "axios": "^1.12.2",
    "boxen": "^5.1.2",
    "chalk": "^4.1.2",
    "cli-progress": "^3.12.0",
    "commander": "^14.0.1",
    "dotenv": "^17.2.2",
    "figures": "^3.2.0",
    "fix": "^0.0.3",
    "fs-extra": "^11.3.2",
    "glob": "^10.4.5",
    "inquirer": "^12.6.3",
    "ora": "^5.4.1",
    "semver": "^7.6.3"
  },
  "devDependencies": {
    "@types/cli-progress": "^3.11.0",
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.8",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.5.2",
    "@types/semver": "^7.5.8",
    "jest": "^30.1.3",
    "supabase": "^2.76.14",
    "ts-jest": "^29.4.4",
    "tsx": "^4.20.6",
    "typescript": "^5.9.2"
  }
}
